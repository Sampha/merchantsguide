<p id="notice"><%= notice %></p>

<h1>Calculations</h1>

<table>
  <thead>
    <tr>
      <th>Value one</th>
      <th>Value two</th>
      <th>Value three</th>
      <th>Value four</th>
      <th>Value one name</th>
      <th>Value two name</th>
      <th>Value three name</th>
      <th>Value four name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calculations.each do |calculation| %>
      <tr>
        <td><%= calculation.value_one %></td>
        <td><%= calculation.value_two %></td>
        <td><%= calculation.value_three %></td>
        <td><%= calculation.value_four %></td>
        <td><%= calculation.value_one_name %></td>
        <td><%= calculation.value_two_name %></td>
        <td><%= calculation.value_three_name %></td>
        <td><%= calculation.value_four_name %></td>
        <td><%= link_to 'Show', calculation %></td>
        <td><%= link_to 'Edit', edit_calculation_path(calculation) %></td>
        <td><%= link_to 'Destroy', calculation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calculation', new_calculation_path %>
